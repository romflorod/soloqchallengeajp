  {
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "img/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "app.py"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ],
    "functions": {
      "app.py": {
        "maxDuration": 60
      }
    }
  }